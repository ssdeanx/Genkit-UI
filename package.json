{
  "name": "genkit-ui",
  "version": "1.0.3",
  "description": "",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssdeanx/genkit-ui.git"
  },
  "readme": "README.md",
  "author": "ssdeanx",
  "keywords": [
    "genkit",
    "genai",
    "google",
    "vertexai",
    "firebase",
    "functions",
    "hosting",
    "agents",
    "a2a",
    "toolbox"
  ],
  "license": "ISC",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "genkit:ui": "dotenvx run -- genkit start -- npx tsx --watch src/index.ts",
    "flow:serve": "dotenvx run -- npx tsx src/flowServer.ts",
    "flow:serve:dev": "dotenvx run -- npx tsx --watch src/flowServer.ts",
    "a2a:cli": "dotenvx run -- npx tsx src/cli.ts",
    "agents:coder": "dotenvx run -- npx tsx src/agents/coder/index.ts",
    "agents:content-editor": "dotenvx run -- npx tsx src/agents/content-editor/index.ts",
    "agents:orchestrator": "dotenvx run -- npx tsx src/agents/orchestrator-agent/index.ts",
    "agents:planning": "dotenvx run -- npx tsx src/agents/planning-agent/index.ts",
    "agents:academic": "dotenvx run -- npx tsx src/agents/academic-research-agent/index.ts",
    "agents:news": "dotenvx run -- npx tsx src/agents/news-research-agent/index.ts",
    "agents:data-analysis": "dotenvx run -- npx tsx src/agents/data-analysis-agent/index.ts",
    "agents:web-research": "dotenvx run -- npx tsx src/agents/web-research-agent/index.ts",
    "deploy": "firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting:apphosting",
    "emulators": "firebase emulators:start",
    "emulators:functions": "firebase emulators:start --only functions",
    "build:functions": "cd functions && npm run build",
    "lint": "eslint --ext .js,.ts src/",
    "lint:fix": "eslint --ext .js,.ts src/ --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
